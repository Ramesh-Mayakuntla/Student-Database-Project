class Hotel
{
	Food orderFood(int choice)
	{
		if(choice ==1)
		{
		Dosa d=new Dosa();
		return d;
		}
		else if(choice==2)
		{
			Idly i=new Idly();
			return i;
		}
		else if(choice==3)
		{
			LemonRice l=new LemonRice();
			return l;
		}
		else if(choice==4)
		{
			Poori p=new Poori();
			return p;
		}
		else if(choice==5)
		{
			Chapati c=new Chapati();
			return c;
		}
		else if(choice==6)
		{
			Pongal po=new Pongal();
			return po;
		}
		else
		{
			return null;
		}
		
	}
	
}
